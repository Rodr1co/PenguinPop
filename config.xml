<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns = "http://www.w3.org/ns/widgets" xmlns:gap = "http://phonegap.com/ns/1.0" id = "com.bigdogmultimedia.bouncing-penguin" version = "1.0.0">
<name>Penguin Pop</name>
<description>This is my second phonegap build project using my Penguin Pop game. Built for my iPhone 4S</description>
<author href="http://bouncing-penguin.bigdogmultimedia.com" email="rod@prowebmaker.com">Roderick Snyder</author>
<feature name="http://api.phonegap.com/1.0/device" />
<preference name="phonegap-version" value="2.1.0" />
<preference name="orientation" value="portrait" />
<preference name="target-device" value="universal" />
<preference name="fullscreen" value="false" />
<preference name="webviewbounce" value="false" />
<preference name="prerendered-icon" value="true" />
<preference name="autorotate" value="false" readonly="true"/> 
<icon src="icon.png" />
<gap:splash src="assets/penguinscreen.png" gap:platform="ios" width="320" height="460" />
</widget>